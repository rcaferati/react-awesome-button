{
  "name": "@rcaferati/react-awesome-button",
  "version": "8.0.0",
  "description": "Performant, extendable, highly customisable, production ready React Component that renders an animated basic set of UI buttons",
  "author": "Rafael Caferati",
  "license": "MIT",
  "homepage": "https://github.com/rcaferati/react-awesome-button",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcaferati/react-awesome-button.git"
  },
  "bugs": {
    "url": "https://github.com/rcaferati/react-awesome-button/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "style": "./dist/styles.css",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css",
    "dist/themes/**/*.js"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./AwesomeButton": {
      "types": "./dist/AwesomeButton.d.ts",
      "import": "./dist/AwesomeButton.mjs",
      "require": "./dist/AwesomeButton.js",
      "default": "./dist/AwesomeButton.mjs"
    },
    "./AwesomeButtonProgress": {
      "types": "./dist/AwesomeButtonProgress.d.ts",
      "import": "./dist/AwesomeButtonProgress.mjs",
      "require": "./dist/AwesomeButtonProgress.js",
      "default": "./dist/AwesomeButtonProgress.mjs"
    },
    "./AwesomeButtonSocial": {
      "types": "./dist/AwesomeButtonSocial.d.ts",
      "import": "./dist/AwesomeButtonSocial.mjs",
      "require": "./dist/AwesomeButtonSocial.js",
      "default": "./dist/AwesomeButtonSocial.mjs"
    },
    "./styles.css": "./dist/styles.css",
    "./dist/styles.css": "./dist/styles.css",
    "./themes/*": "./dist/themes/*.js",
    "./dist/themes/*": "./dist/themes/*.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "AwesomeButton": [
        "dist/AwesomeButton.d.ts"
      ],
      "AwesomeButtonProgress": [
        "dist/AwesomeButtonProgress.d.ts"
      ],
      "AwesomeButtonSocial": [
        "dist/AwesomeButtonSocial.d.ts"
      ]
    }
  },
  "scripts": {
    "start": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "npm run build:all",
    "build:js": "tsup --config tsup.config.ts",
    "build:js:legacy": "webpack --mode production",
    "build:scss": "webpack --mode production --config webpack.scss.config.js && node -e \"require('fs').rmSync('./dist/styles.js', { force: true })\"",
    "build:themes": "babel-node ./theme-builder.js",
    "build:all": "npm run build:js && npm run build:scss && npm run build:themes",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "prepublishOnly": "npm run build:all"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@rcaferati/wac": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-a11y": "^10.2.11",
    "@storybook/addon-docs": "^10.2.11",
    "@storybook/addon-onboarding": "^10.2.11",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react-webpack5": "^10.2.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.2.0",
    "@types/react": "^18.0.21",
    "@types/react-test-renderer": "^18.0.0",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^30.2.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.11.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-storybook": "^10.2.11",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lucide-react": "^0.575.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^7.3.4",
    "prismjs": "^1.29.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.97.3",
    "sass-loader": "^13.3.3",
    "shelljs": "^0.8.5",
    "storybook": "^10.2.11",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.4.1",
    "tsup": "^8.5.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "keywords": [
    "react-component",
    "react-awesome-button",
    "@rcaferati/react-awesome-button",
    "react",
    "button",
    "awesome-button",
    "progress",
    "component",
    "ui-component",
    "javascript"
  ],
  "volta": {
    "node": "24.13.1"
  }
}
